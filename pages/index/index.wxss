
.indexContainer {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  background: blueviolet;
}

.Img {
  width: 200rpx;
  height: 200rpx;
  border-radius: 45rpx;
  margin: 100rpx 0;
}

.user {
  font-size: 32rpx;
  font-weight: bold;
  margin: 100rpx 0;
}

.Study {
  width: 300rpx;
  height: 80rpx;
  border: 1rpx solid #eee;
  text-align: center;
  line-height: 80rpx;
  border-radius: 10rpx;
  font-size: 28rpx;
}